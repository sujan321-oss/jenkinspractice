pipeline {
    agent any
    stages {
        
        stage('Build') {
            steps {
                sh "echo 'building the code'"  // Make sure the step is properly defined with curly braces
            }
        }

        stage("Testing") {
            steps {
                sh "echo 'testing the code'"  // Same here: define steps with curly braces
            }
        }
    }
}
